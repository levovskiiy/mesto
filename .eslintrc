{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "arrow-parens": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": true, "variables": true }]
  },
  "extends": ["airbnb", "prettier","eslint:recommended"]
}
